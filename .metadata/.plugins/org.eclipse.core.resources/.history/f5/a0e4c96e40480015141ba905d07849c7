package com.maxbilbow.aicubo;

import click.rmx.engine.Node;
import click.rmx.engine.Scene;
import click.rmx.engine.math.Vector3;

public abstract class EntityGenerator {
	public abstract Node makeEntity();
	public int 
	xMin, yMin, zMin,
	xMax, yMax, zMax;
	
	public EntityGenerator() {
		
	}
	protected void initPositionOf(Node node) {
		node.transform.setPosition(Math.random() % 1,);
	}
	public void makeShapesAndAddToScene(Scene scene, int count) {
		
	}
}
