package click.rmx.engine;

import java.util.LinkedList;

public class CollisionBody extends NodeComponent {

	boolean niaveMatch(Node nodeA, Node nodeB) {
		return false;
	}
	
	public CollisionEvent checkForCollisions(CollisionBody potentialCollision) {
		
		return new CollisionEvent(this.getNode(), potentialCollision.getNode());
	}
}
